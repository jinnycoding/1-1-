//백준 2775번
import java.util.Scanner;
public class Main{
  public static void main(String[] args){
    Scanner in = new Scanner(System.in);
    
    int[][] Apt = new int[15][15];
    
    for(int i = 0; i < 15; i++){
      Apt[i][1] = 1;
      Apt[0][i] = i;
    }
    
    for(int i = 1; i<15; i++){
      for (int j =2; j<15; j++){
        Apt[i][j] = Apt[i][j-1]
      }
    }
  }
}
