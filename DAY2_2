//두 정수 A와 B가 주어졌을 때, A와 B를 비교하는 프로그램을 작성하시오.
import java,util.Scanner;
public class Main{
  public static void main(String[] args){
    Scanner in = new Scanner(System.in);
    int A = in.nextIn();
    int A = in.nextIn();
    in.close();
    
    if (A>B){
    System.out.println(">");
    } else if(A<B){
    System.out.println("<");
    } else{
    System.out.println("==");
    }
  }
}
